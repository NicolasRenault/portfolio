{
    "name": "portfolio",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "npm run i18n:sync && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "github:project": "ts-node --esm scripts/generate-project-list.ts",
        "i18n:install": "astro-i18n install",
