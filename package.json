    "dependencies": {
        "@octokit/core": "^4.2.4",
        "astro": "^3.0.7",
        "astro-i18n": "^1.8.1",
        "astro-icon": "^0.8.1",
        "autoprefixer": "^10.4.14",
        "sass": "^1.62.1",
        "ts-node": "^10.9.1"
    },
    "devDependencies": {
        "@types/node": "^20.4.1",
        "postcss-html": "^1.5.0",
        "prettier": "^2.8.8",
        "prettier-plugin-astro": "^0.9.0"
    }
}
